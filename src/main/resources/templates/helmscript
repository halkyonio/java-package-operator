cat << EOF > values.yml
{s.helmValues}
EOF

helm repo add {s.helm.chart.name} {s.helm.chart.repoUrl}
helm repo update

helm install {s.helm.chart.name} {s.helm.chart.name}/{s.helm.chart.name}{#if s.version??} --version {s.version}{/}{#if s.namespace??} --namespace {s.namespace}{/}{#if s.createNamespace??} --create-namespace{/} -f values.yml
